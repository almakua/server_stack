# Environment Variables
# Copy this file to .env in the aragorn root directory
#
# Get API keys from:
# - Jellyfin: Dashboard -> API Keys -> Add
# - Sonarr/Radarr/Lidarr/Bazarr/Prowlarr: Settings -> General -> API Key
# - qBittorrent: use your login password

# Pi-hole
PIHOLE_WEBPASSWORD=your_pihole_password

# Homepage widgets
HOMEPAGE_VAR_JELLYFIN_API_KEY=your_jellyfin_api_key
HOMEPAGE_VAR_QBITTORRENT_PASSWORD=your_qbittorrent_password
HOMEPAGE_VAR_PROWLARR_API_KEY=your_prowlarr_api_key
HOMEPAGE_VAR_SONARR_API_KEY=your_sonarr_api_key
HOMEPAGE_VAR_RADARR_API_KEY=your_radarr_api_key
HOMEPAGE_VAR_LIDARR_API_KEY=your_lidarr_api_key
HOMEPAGE_VAR_BAZARR_API_KEY=your_bazarr_api_key
HOMEPAGE_VAR_PIHOLE_API_KEY=your_pihole_api_key
HOMEPAGE_VAR_JELLYSEERR_API_KEY=your_jellyseerr_api_key

